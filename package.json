{
	"name": "rn-liquid-design",
	"version": "0.1.7",
	"description": "React Native UI Library by Merck",
	"source": "./src/index.js",
	"main": "./lib/commonjs/index.js",
	"module": "./lib/module/index.js",
	"types": "./src/types",
	"exports": {
		".": {
			"import": {
				"default": "./lib/module/index.js"
			},
			"require": {
				"default": "./lib/commonjs/index.js"
			}
		}
	},
	"files": [
		"src",
		"lib",
		"android",
		"ios",
		"cpp",
		"*.podspec",
		"react-native.config.json",
		"!ios/build",
		"!android/build",
		"!android/gradle",
		"!android/gradlew",
		"!android/gradlew.bat",
		"!android/local.properties",
		"!**/__tests__",
		"!**/__fixtures__",
		"!**/__mocks__",
		"!**/.*"
	],
	"scripts": {
		"example": "yarn workspace rn-liquid-design-example",
		"test": "jest",
		"typecheck": "tsc",
		"lint": "eslint \"**/*.{js,ts,tsx}\"",
		"clean": "del-cli lib",
		"prepare": "bob build",
		"release": "release-it"
	},
	"keywords": [
		"react-native",
		"ios",
		"android"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/m-turjoy/rn-liquid-design.git"
	},
	"author": "m-turjoy <turjoy.saha@external.milliporesigma.com> (https://github.com/m-turjoy)",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/m-turjoy/rn-liquid-design/issues"
	},
	"homepage": "https://github.com/m-turjoy/rn-liquid-design#readme",
	"publishConfig": {
		"registry": "https://registry.npmjs.org/"
	},
	"dependencies": {
		"deprecated-react-native-prop-types": "^5.0.0",
		"lodash": "^4.17.21",
		"moment": "^2.30.1",
		"prop-types": "^15.6.2",
		"react-dom": "^18.3.1",
		"react-native-gesture-handler": "^2.20.2",
		"react-native-modal-dropdown": "^1.0.2",
		"react-native-progress": "^5.0.1",
		"react-native-reanimated": "3.10.1",
		"react-native-root-siblings": "^5.0.1",
		"react-native-side-menu": "^1.1.3",
		"react-native-svg": "^15.8.0",
		"redux": "^4.0.0",
		"snyk": "^1.1294.0",
		"styled-components": "^6.1.13",
		"styled-system": "^2.3.2",
		"styled-theming": "^2.2.0",
		"uuid": "11.0.0",
		"victory-create-container": "^37.3.0",
		"victory-native": "^41.8.0",
		"xdate": "^0.8.3"
	},
	"devDependencies": {
		"@babel/core": "^7.26.0",
		"@babel/preset-env": "^7.26.0",
		"@commitlint/config-conventional": "^17.0.2",
		"@evilmartians/lefthook": "^1.5.0",
		"@react-native/eslint-config": "^0.73.1",
		"@release-it/conventional-changelog": "^5.0.0",
		"@shopify/react-native-skia": "^1.5.0",
		"@types/jest": "^29.5.5",
		"@types/react": "^18.2.44",
		"commitlint": "^17.0.2",
		"del-cli": "^5.1.0",
		"eslint": "^8.51.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-prettier": "^5.0.1",
		"expo-modules-autolinking": "^1.11.3",
		"jest": "^29.7.0",
		"prettier": "^3.0.3",
		"react": "18.2.0",
		"react-native": "0.74.5",
		"react-native-builder-bob": "^0.30.2",
		"release-it": "^15.0.0",
		"typescript": "^5.2.2"
	},
	"resolutions": {
		"@types/react": "^18.2.44"
	},
	"peerDependencies": {
		"react": "*",
		"react-native": "*"
	},
	"workspaces": [
		"example"
	],
	"packageManager": "yarn@3.6.1",
	"jest": {
		"preset": "react-native",
		"modulePathIgnorePatterns": [
			"<rootDir>/example/node_modules",
			"<rootDir>/lib/"
		]
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"release-it": {
		"git": {
			"commitMessage": "chore: release ${version}",
			"tagName": "v${version}"
		},
		"npm": {
			"publish": true
		},
		"github": {
			"release": true
		},
		"plugins": {
			"@release-it/conventional-changelog": {
				"preset": "angular"
			}
		}
	},
	"eslintConfig": {
		"root": true,
		"extends": [
			"@react-native",
			"prettier"
		],
		"rules": {
			"react/react-in-jsx-scope": "off",
			"prettier/prettier": [
				"error",
				{
					"quoteProps": "consistent",
					"singleQuote": false,
					"tabWidth": 2,
					"trailingComma": "es5",
					"useTabs": true
				}
			]
		}
	},
	"eslintIgnore": [
		"node_modules/",
		"lib/"
	],
	"prettier": {
		"quoteProps": "consistent",
		"singleQuote": false,
		"tabWidth": 2,
		"trailingComma": "es5",
		"useTabs": true
	},
	"react-native-builder-bob": {
		"source": "src",
		"output": "lib",
		"targets": [
			[
				"commonjs",
				{
					"esm": true
				}
			],
			[
				"module",
				{
					"esm": true
				}
			],
			[
				"typescript",
				{
					"project": "tsconfig.build.json",
					"esm": true
				}
			]
		]
	},
	"create-react-native-library": {
		"type": "library",
		"version": "0.42.1"
	}
}
